A,GAAA,GAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,CAAA,CAAA,QAAA;AACA;AACA,OAPA;AAQA,KAVA;AAWA,IAAA,SAXA,qBAWA,CAXA,EAWA,CAXA,EAWA;AAAA;;AACA,UAAA,GAAA,GAAA,CAAA,CAAA,MAAA;AACA,UAAA,GAAA,CAAA,SAAA,CAAA,QAAA,CAAA,UAAA,CAAA,EAAA;;AAEA,UAAA,CAAA,KAAA,QAAA,EAAA;AACA,aAAA,QAAA,CAAA,eAAA,EAAA,IAAA,EAAA;AACA,UAAA,iBAAA,EAAA,IADA;AAEA,UAAA,gBAAA,EAAA,KAFA;AAGA,UAAA,IAAA,EAAA;AAHA,SAAA,EAKA,IALA,CAKA,YAAA;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,YAAA,IAAA,EAAA,OADA;AAEA,YAAA,KAAA,EAAA;AACA,cAAA,QAAA,EAAA;AADA;AAFA,WAAA;AAMA,SAZA,EAaA,KAbA,CAaA,YAAA,CAAA,CAbA;AAeA;AACA;;AAEA,MAAA,SAAA,CAAA;AACA,QAAA,MAAA,EAAA,KAAA,EADA;AAEA,QAAA,IAAA,EAAA,OAAA;AAFA,OAAA,CAAA,CAGA,IAHA,CAGA,UAAA,GAAA,EAAA;AACA,YAAA,CAAA,GAAA,SAAA,IAAA,GAAA,WAAA,YAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AAAA;;AACA,cAAA,EAAA,gBAAA,GAAA,CAAA,IAAA,kEAAA,UAAA,MAAA,qDAAA,iBAAA,EAAA;AACA,cAAA,kBAAA,iBAAA,GAAA,CAAA,IAAA,wEAAA,WAAA,UAAA,0DAAA,sBAAA,kBAAA;AACA,cAAA,IAAA,GAAA,CAAA,IAAA,CAAA,GAAA,KAAA,GAAA,KAAA;AACA,cAAA,YAAA,iBAAA,GAAA,CAAA,IAAA,wEAAA,WAAA,UAAA,0DAAA,sBAAA,YAAA;AACA,cAAA,IAAA,iBAAA,GAAA,CAAA,IAAA,wEAAA,WAAA,UAAA,0DAAA,sBAAA,IAAA;;AAEA,UAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA,YAAA,IAAA,EAAA,QADA;AAEA,YAAA,KAAA,EAAA;AACA,cAAA,GAAA,EAAA,CADA;AAEA,cAAA,QAAA,EAAA,EAFA;AAGA,cAAA,kBAAA,EAAA,kBAHA;AAIA,cAAA,IAAA,EAAA,IAJA;AAKA,cAAA,YAAA,EAAA,YALA;AAMA,cAAA,IAAA,EAAA;AANA;AAFA,WAAA;AAWA;AACA,OAvBA,EAvBA,CAgDA;AACA;AACA;AA7DA;AAvBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"newslist ql-editor\" v-if=\"visible\">\r\n      <div class=\"tle\" style=\"text-align: center;margin-top: 10px;\">\r\n        <h3>{{title}}</h3>\r\n      </div>\r\n      <div\r\n        class=\"content\"\r\n        style=\"padding: 10px 15px 10px 15px;line-height: 30px;\"\r\n        v-html=\"contents\"\r\n      >\r\n      </div>\r\n      <div class=\"foot\">\r\n        <div class=\"btn mr50 \" @click=\"toDeclare($event, 1)\">创新类在线申报</div>\r\n        <div class=\"btn \" @click=\"toDeclare($event, 2)\">创业类在线申报</div>\r\n      </div>\r\n    </div>\r\n    <div v-else style=\"text-align:left;\">\r\n      <innovate v-if=\"tag == 1\"></innovate>\r\n      <business v-else-if=\"tag == 2\"></business>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {getArticle, cityCheck} from '../../../api/talentReport.js';\r\nimport innovate from './innovate.vue';\r\nimport business from './business.vue';\r\nimport { getId } from \"@/utils/auth\";\r\nimport { mapState } from 'vuex';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      title: '',\r\n      contents: '',\r\n      visible: true, // 是否显示表格\r\n      tag: 1,\r\n    }\r\n  },\r\n  components: {\r\n    innovate,\r\n    business,\r\n  },\r\n  created() {\r\n    if(this.$route.query.tag) {\r\n      this.visible = false\r\n      this.tag = this.$route.query.tag\r\n    }\r\n    this.query()\r\n  },\r\n  computed: {\r\n    ...mapState(['userInfo'])\r\n  },\r\n  methods: {\r\n    query() {\r\n      getArticle('62805fe3-5772-42f4-9cb7-46e2c6c69a84').then(res => {\r\n        console.log(res);\r\n        if(res?.data) {\r\n          let d = res.data\r\n          this.title = d.title\r\n          this.contents = d.contents\r\n        }\r\n      })\r\n    },\r\n    toDeclare(e, i) {\r\n      let dom = e.target\r\n      if(dom.classList.contains('disabled')) return\r\n\r\n      if(!this.userInfo) {\r\n        this.$confirm(\"当前用户还未登录，请先登录\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        showCancelButton: false,\r\n        type: \"warning\"\r\n      })\r\n        .then(() => {\r\n          this.$router.push({\r\n            name: \"login\",\r\n            query: {\r\n              redirect: \"cTlent\"\r\n            }\r\n          });\r\n        })\r\n        .catch(() => {});\r\n\r\n        return\r\n      }\r\n\r\n      cityCheck({\r\n        userId: getId(),\r\n        type: 'lj'+i\r\n      }).then(res => {\r\n        if(res?.code == 200) {\r\n          let id = res.data?.talent?.id\r\n          let cityEnterpriseType = res.data?.enterprise?.cityEnterpriseType\r\n          let type = i == 1 ? 'lj1' : 'lj2'\r\n          let districtName = res.data?.enterprise?.districtName\r\n          let name = res.data?.enterprise?.name\r\n\r\n          this.$router.replace({\r\n            name: 'cTlent',\r\n            query: {\r\n              tag: i,\r\n              talentId: id,\r\n              cityEnterpriseType,\r\n              type,\r\n              districtName,\r\n              name\r\n            }\r\n          })\r\n        }\r\n      })\r\n\r\n      // this.tag = i\r\n      // this.visible = false\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.foot {\r\n  display: flex;\r\n  justify-content: center;\r\n  .btn {\r\n    padding: 10px 20px;\r\n    background: rgba(22, 155, 213, 1);\r\n    color: #fff;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n    &.mr50 {\r\n      margin-right: 50px;\r\n    }\r\n    &.disabled {\r\n      background: #ccc;\r\n      cursor: not-allowed;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/talentReport/components"}]}