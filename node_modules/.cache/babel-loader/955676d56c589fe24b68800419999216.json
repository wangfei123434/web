ss=\"banner\">\r\n      <img\r\n        class=\"banner-image\"\r\n        src=\"../assets/imgs/wfimg/home-banner.png\"\r\n        alt=\"\"\r\n      />\r\n    </div>\r\n    <!-- 通知公告 -->\r\n    <div class=\"notice-box\">\r\n      <div class=\"notice-l\">\r\n        <div class=\"txt\">\r\n          <img\r\n            class=\"banner-image\"\r\n            src=\"../assets/imgs/wfimg/horn.png\"\r\n            alt=\"\"\r\n          />\r\n          通知公告\r\n        </div>\r\n        <div class=\"icon\"></div>\r\n      </div>\r\n      <div class=\"notice-r\">\r\n        <div class=\"swiper\">\r\n          <div class=\"swiper-wrapper\">\r\n            <div\r\n              class=\"swiper-slide\"\r\n              v-for=\"(item, index) in announcementsList\"\r\n              :key=\"index\"\r\n            >\r\n              <div class=\"txt\">\r\n                {{ item.title }}<span>{{ item.postDate }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"notice-more\">查看更多 ></div>\r\n    </div>\r\n    <!-- 资讯中心 -->\r\n    <div class=\"information-box\">\r\n      <div class=\"information-box-c\">\r\n        <div class=\"titleImg-box\">\r\n          <img src=\"../assets/imgs/wfimg/zxzx-t-img.png\" alt=\"\" />\r\n        </div>\r\n        <div class=\"information-box-list\">\r\n          <dir class=\"information-item\" v-for=\"(item, index) in this.newsData.人才新闻\" :key=\"index\" v-if=\"index === 0 \">\r\n            <img src=\"../assets/imgs/wfimg/home-banner.png\" alt=\"\" />\r\n            <div class=\"item-box1\">\r\n              {{item.title}}\r\n            </div>\r\n            <div class=\"item-box2\">\r\n              <div class=\"item-box2-l\">{{item.dateCreated.substring(0,11)}}</div>\r\n              <div class=\"item-box2-r\">了解详情 ></div>\r\n            </div>\r\n            <div class=\"item-box3\">\r\n              {{item.title}}\r\n            </div>\r\n            <div class=\"icon-box\">{{ informationList[0] }}</div>\r\n          </dir>\r\n          <dir class=\"information-item\" v-for=\"(item, index) in this.newsData.工作动态\" :key=\"index\" v-if=\"index === 0 \">\r\n            <img src=\"../assets/imgs/wfimg/home-banner.png\" alt=\"\" />\r\n            <div class=\"item-box1\">\r\n              {{item.title}}\r\n            </div>\r\n            <div class=\"item-box2\">\r\n              <div class=\"item-box2-l\">{{item.dateCreated.substring(0,11)}}</div>\r\n              <div class=\"item-box2-r\">了解详情 ></div>\r\n            </div>\r\n            <div class=\"item-box3\">\r\n              {{item.title}}\r\n            </div>\r\n            <div class=\"icon-box\">{{ informationList[1] }}</div>\r\n          </dir>\r\n          <dir class=\"information-item\" v-for=\"(item, index) in this.newsData.人才活动\" :key=\"index\" v-if=\"index === 0 \">\r\n            <img src=\"../assets/imgs/wfimg/home-banner.png\" alt=\"\" />\r\n            <div class=\"item-box1\">\r\n              {{item.title}}\r\n            </div>\r\n            <div class=\"item-box2\">\r\n              <div class=\"item-box2-l\">{{item.dateCreated.substring(0,11)}}</div>\r\n              <div class=\"item-box2-r\">了解详情 ></div>\r\n            </div>\r\n            <div class=\"item-box3\">\r\n              {{item.title}}\r\n            </div>\r\n            <div class=\"icon-box\">{{ informationList[2] }}</div>\r\n          </dir>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- 人才政策 -->\r\n    <div class=\"rczc-box\">\r\n      <div class=\"box-c\">\r\n        <div class=\"titleImg-box\">\r\n          <img src=\"../assets/imgs/wfimg/rczc-t-img.png\" alt=\"\" />\r\n        </div>\r\n        <div class=\"line-box\"></div>\r\n        <div class=\"more-box\">查看更多 ></div>\r\n        <div class=\"list-box\">\r\n          <div class=\"item-box\" v-for=\"(item,index) in this.newsData.政策文件\" :key=\"index\" v-if=\"index<4 \">\r\n            <div class=\"item-box-l\">\r\n              <div class=\"l1\">{{item.dateCreated.substring(0,7)}}</div>\r\n              <div class=\"l2\">{{item.dateCreated.substring(8,11)}}</div>\r\n            </div>\r\n            <div class=\"item-box-r\">{{item.title}}</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- 人才申报 -->\r\n    <div class=\"rcsb-box\">\r\n      <div class=\"box-c\">\r\n        <div class=\"titleImg-box\">\r\n          <img src=\"../assets/imgs/wfimg/rcsb-t-img.png\" alt=\"\" />\r\n        </div>\r\n        <div class=\"line-box\"></div>\r\n        <div class=\"more-box\">查看更多 ></div>\r\n        <div class=\"list-box\">\r\n          <div class=\"item-box\" v-for=\"(item,index) in rcsbNavList\" :key=\"index\">\r\n            <img :src=\"item.img\" alt=\"\" srcset=\"\">\r\n            <div class=\"t\">{{item.t}}</div>\r\n            <div class=\"c\">{{item.c}}</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- 人才礼遇 -->\r\n     <div class=\"rcly-box\">\r\n      <div class=\"box-c\">\r\n        <div class=\"titleImg-box\">\r\n          <img src=\"../assets/imgs/wfimg/rcly-t-img.png\" alt=\"\" />\r\n        </div>\r\n        <div class=\"line-box\"></div>\r\n        <div class=\"more-box\">查看更多 ></div>\r\n        <div class=\"list-box\">\r\n          <div class=\"item-box\" v-for=\"(item,index) in rclyNavList\" :key=\"index\">\r\n            <img :src=\"item.img\" alt=\"\">\r\n            <span>{{item.t}}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- 联系我们 -->\r\n    <div class=\"contact-us-box\">\r\n      <div class=\"titleImg-box\">\r\n        <img src=\"../assets/imgs/wfimg/lxwm-t-img.png\" alt=\"\" />\r\n      </div>\r\n      <div class=\"map-box\">\r\n        <div class=\"map-box-l\">\r\n          <div>\r\n            <img src=\"../assets/imgs/wfimg/dz-icon.png\" alt=\"\" />\r\n            <span>地址</span>\r\n          </div>\r\n          <p>武汉市江岸区劳动街办事处惠济路39号杨森公馆</p>\r\n          <div style=\"margin-top:30px\">\r\n            <img src=\"../assets/imgs/wfimg/dz-icon.png\" alt=\"\" />\r\n            <span>联系电话</span>\r\n          </div>\r\n          <p>027-82267029</p>\r\n        </div>\r\n        <div class=\"map-box-r\">\r\n          <img src=\"../assets/imgs/wfimg/map-img.png\" alt=\"\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- 通知栏 -->\r\n    <div ref=\"popUpDom\" class=\"popUpBox\">\r\n      <div class=\"title\">\r\n        <div class=\"leftD\">最新消息</div>\r\n        <div class=\"rightD\" @click=\"isHidePop\">\r\n          <span></span>\r\n        </div>\r\n      </div>\r\n      <div class=\"messageContent\">\r\n        <div class=\"t\">尊敬的人才服务网用户您好！</div>\r\n        <div class=\"c\">\r\n          人才安居服务系统已于近期更新完成，用户可通过原账号直接登录。大学生就业创业类服务事项可在服务大厅相应栏目查看。\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Swiper from \"swiper\";\r\nimport { swiper, swiperSlide } from \"vue-awesome-swiper\";\r\nimport \"swiper/dist/css/swiper.css\";\r\nimport { homeData } from \"../api/home\";\r\nimport { getAllArticle } from \"../api/other.js\";\r\n\r\nexport default {\r\n  name: \"Home\",\r\n  components: {\r\n    swiper,\r\n    swiperSlide\r\n  },\r\n  data() {\r\n    return {\r\n      announcementsList: [\r\n        {\r\n          title: \"2021年度“武汉英才”举荐申报公告\",\r\n          postDate: \"2021\"\r\n        },\r\n        {\r\n          title: \"2022年度“武汉英才”举荐申报公告\",\r\n          postDate: \"2022\"\r\n        }\r\n      ], //通知公告列表\r\n      mySwiper: \"\",\r\n      informationList: [\"人才要闻\", \"工作动态\", \"人才活动\"],\r\n      rcsbNavList:[\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcsb-zlkjrc.png\"),\r\n          t:'武汉英才',\r\n          c:\"战略科技人才\"\r\n        },\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcsb-cyljrc.png\"),\r\n          t:'武汉英才',\r\n          c:'产业领军人才'\r\n        },\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcsb-yxqnrc.png\"),\r\n          t:'武汉英才',\r\n          c:'优秀青年人才'\r\n        },\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcsb-zygcsrc.png\"),\r\n          t:'武汉英才',\r\n          c:\"卓越工程师人才\"\r\n        },\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcsb-pyzczx.png\"),\r\n          t:'武汉英才',\r\n          c:\"培育支持专项\"\r\n        },\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcsb-tszx.png\"),\r\n          t:'武汉英才',\r\n          c:\"特色专项\"\r\n        },\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcsb-rcjh.png\"),\r\n          t:'3551光谷',\r\n          c:\"人才计划\"\r\n        }\r\n      ],\r\n      rclyNavList: [\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcly-rclh.png\"),\r\n          t:'人才落户'\r\n        },\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcly-rcaj.png\"),\r\n          t:'人才安居'\r\n        },\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcly-sxjx.png\"),\r\n          t:'实习见习'\r\n        },\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcly-jyfw.png\"),\r\n          t:'就业服务'\r\n        },\r\n        {\r\n          img: require(\"@/assets/imgs/wfimg/home-rcly-cxcy.png\"),\r\n          t:'创新创业'\r\n        },\r\n      ],\r\n      newsData: null\r\n    };\r\n  },\r\n  created() {},\r\n  computed: {},\r\n  mounted() {\r\n    this.initSwiper();\r\n    setTimeout(() => {\r\n      document.querySelector(\".popUpBox\").classList.add(\"showPop\");\r\n    }, 2000);\r\n    this.getHomeData();\r\n  },\r\n  methods: {\r\n    // swiper初始化\r\n    initSwiper() {\r\n      this.mySwiper = new Swiper(\".swiper\", {\r\n        direction: \"vertical\", // 垂直切换选项\r\n        loop: true, // 循环模式选项\r\n        autoplay: true\r\n      });\r\n    },\r\n    // 右下角弹窗关闭\r\n    isHidePop() {\r\n      this.$refs.popUpDom.classList.remove(\"showPop\");\r\n    },\r\n\r\n    query() {\r\n      getAllArticle().then(res => {\r\n  