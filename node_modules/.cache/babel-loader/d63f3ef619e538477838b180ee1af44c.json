ontents: this.form.desc,\r\n        contact: this.form.tel,\r\n        version: 0\r\n      }).then(res => {\r\n        console.log(res);\r\n        if (res.code == 200) {\r\n          this.form.name= ''\r\n          this.form.tel= ''\r\n          this.form.subject= ''\r\n          this.form.desc= ''\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"提交成功\"\r\n          });\r\n\r\n        }\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.InteractiveCommunication{\r\n  box-shadow: 0px 2px 10px rgba(0,0,0,0.1600);\r\n}\r\n.introduction-body{\r\n  font-size: 18px;\r\n  font-weight: 400;\r\n  line-height: 42px;\r\n  color: #666666;\r\n  width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n.submit-button{\r\n  width: 220px;\r\n  height: 60px;\r\n  background: #0A63DD;\r\n  font-size: 18px;\r\n  font-weight: 400;\r\n  color: #FFFFFF;\r\n}\r\n.demo-ruleForm{\r\n  width: 80%;\r\n}\r\n/deep/ .el-form-item{\r\n  margin-bottom: 20px;\r\n}\r\n/deep/ .el-input{\r\n  &,& .el-input__inner,.el-input__icon,.el-select__caret{\r\n    height: 46px!important;\r\n    line-height: 46px;\r\n    border-radius: 0px;\r\n  }\r\n}\r\n/deep/ .el-form-item__label,/deep/ .el-input textarea{\r\n  line-height: 46px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/gywm/components"}]}