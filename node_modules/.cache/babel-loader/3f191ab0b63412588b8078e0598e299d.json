AA,CAAA,kBAAA,EAAA,YAAA;;AACA,UAAA,IAAA,GAAA,EAAA,EAAA;AACA,QAAA,QAAA,CAAA,aAAA,CAAA,kBAAA,EAAA,KAAA,CAAA,MAAA,GAAA,EAAA,GAAA,IAAA;AACA,QAAA,QAAA,CAAA,aAAA,CAAA,kBAAA,EAAA,SAAA,CAAA,GAAA,CAAA,MAAA;AACA;AACA,KAZA;AAaA,IAAA,eAbA,6BAaA;AAAA;;AACA,MAAA,UAAA,CAAA,KAAA,EAAA,CAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,YAAA,CAAA,GAAA,IAAA,CAAA,IAAA;;AACA,YAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,GAAA,CAAA,CAAA,QAAA;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,CAAA,CAAA,MAAA,CAFA,CAGA;;AACA,UAAA,KAAA,CAAA,WAAA,GACA,CAAA,CAAA,QAAA,IAAA,CAAA,CAAA,QAAA,IAAA,cAAA,GACA,MAAA,CAAA,CAAA,CAAA,QAAA,CAAA,CAAA,MAAA,CAAA,YAAA,CADA,GAEA,EAHA;AAIA,UAAA,KAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;AACA,UAAA,KAAA,CAAA,MAAA,GAAA,CAAA,CAAA,MAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,CAAA,CAAA,IAAA,CAVA,CAYA;;AACA,UAAA,UAAA,CAAA,YAAA;AACA,YAAA,KAAA,CAAA,cAAA;AACA,WAFA,EAEA,GAFA,CAAA;AAGA;AACA,OApBA;AAqBA;AAnCA;AAlBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"content-wrapper ql-snow\">\r\n    <h2 style=\"text-align: center;\">{{ title }}</h2>\r\n    <p style=\"text-align: center;\">\r\n      发布时间： <span>{{ dateCreated }}</span> 作者：<span>{{ author }}</span>\r\n      来源：<span>{{ source }}</span> 浏览次数：<span>{{ hits }}</span>\r\n    </p>\r\n    <div v-html=\"contents\" class=\"ql-editor\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getArticle } from \"../../../api/other.js\";\r\nimport moment from \"moment\";\r\nimport \"quill/dist/quill.snow.css\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      id: \"\",\r\n      contents: \"\",\r\n      author: \"\",\r\n      dateCreated: \"\",\r\n      title: \"\",\r\n      source: \"\",\r\n      hits: \"\"\r\n    };\r\n  },\r\n  created() {\r\n    console.log(this.$route.query.id);\r\n    this.id = this.$route.query.id;\r\n    this.getLittleBanner();\r\n  },\r\n  mounted() {},\r\n  methods: {\r\n    calcFullHeight() {\r\n      let h = document.documentElement.clientHeight;\r\n      let headH = document.querySelector(\".header-wrapper\").clientHeight;\r\n      let footH = document.querySelector(\"footer\").clientHeight;\r\n\r\n      let hh = h - headH - footH - 20;\r\n      let boxH = document.querySelector(\".content-wrapper\").clientHeight;\r\n      if (boxH < hh) {\r\n        document.querySelector(\".content-wrapper\").style.height = hh + \"px\";\r\n        document.querySelector(\".content-wrapper\").classList.add(\"auto\");\r\n      }\r\n    },\r\n    getLittleBanner() {\r\n      getArticle(this.id).then(data => {\r\n        console.log(data);\r\n        let d = data.data;\r\n        if (d) {\r\n          this.contents = d.contents;\r\n          this.author = d.author;\r\n          // this.dateCreated = d.dateCreated;\r\n          this.dateCreated =\r\n            d.postDate && d.postDate != \"Invalid Date\"\r\n              ? moment(d.postDate).format(\"yyyy-MM-DD\")\r\n              : \"\";\r\n          this.title = d.title;\r\n          this.source = d.source;\r\n          this.hits = d.hits;\r\n\r\n          // 计算满屏高度\r\n          setTimeout(() => {\r\n            this.calcFullHeight();\r\n          }, 500);\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n// 版心\r\n.content-wrapper {\r\n  width: 1200px;\r\n  margin: 0 auto;\r\n  .auto {\r\n    overflow: auto;\r\n  }\r\n  h2 {\r\n    font-size: 24px;\r\n    font-family: Source Han Sans CN;\r\n    font-weight: bold;\r\n    color: #273140;\r\n    padding-top: 40px;\r\n  }\r\n  p{\r\n    font-size: 14px;\r\n    font-family: Source Han Sans CN;\r\n    font-weight: 400;\r\n    color: #999999;\r\n    margin: 20px 0;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/zxzx/detailPage"}]}